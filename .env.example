# Database URLs
# Production database (pooled connection via Prisma Accelerate)
DATABASE_URL="postgres://user:password@host/database?pgbouncer=true"

# Direct database connection (for migrations)
DATABASE_URL_UNPOOLED="postgres://user:password@host/database"

# Preview database (for preview deployments)
# Set this in Vercel dashboard for preview environments only
PREVIEW_DATABASE_URL="postgres://user:password@host/preview_database?pgbouncer=true"

# NextAuth (for future authentication)
NEXTAUTH_URL="https://yourdomain.com"
NEXTAUTH_SECRET="generate-a-secret-key-here"

# Application
NODE_ENV="production"
NEXT_PUBLIC_APP_URL="https://yourdomain.com"

# Optional: Error tracking
SENTRY_DSN=""
SENTRY_AUTH_TOKEN=""

# Optional: Analytics
NEXT_PUBLIC_GOOGLE_ANALYTICS=""
NEXT_PUBLIC_VERCEL_ANALYTICS_ID=""

# AI Services
OPENAI_API_KEY=""

# Email Service (Resend)
# Get your API key from https://resend.com/api-keys
RESEND_API_KEY="re_xxxxxxxxxxxxxxxxx"
EMAIL_FROM="meetings@yourdomain.com"

