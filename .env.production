# Production Environment Variables Template
# Copy this to .env.production.local and fill in your production values

# Database
DATABASE_URL="your-production-database-url"
PRISMA_DATABASE_URL="your-production-prisma-accelerate-url"
POSTGRES_URL="your-production-postgres-url"

# Authentication
NEXTAUTH_URL="https://your-production-domain.com"
NEXTAUTH_SECRET="generate-a-secure-secret-for-production"

# Email Service
RESEND_API_KEY="your-production-resend-api-key"
EMAIL_FROM="meetings@your-production-domain.com"

# Application
NEXT_PUBLIC_APP_URL="https://your-production-domain.com"

# AI Services
OPENAI_API_KEY="your-production-openai-api-key"

# Feature Flags (Optional)
NEXT_PUBLIC_ENABLE_ANALYTICS="true"
NEXT_PUBLIC_ENABLE_DEBUG="false"